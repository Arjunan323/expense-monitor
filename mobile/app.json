{
  "expo": {
    "name": "CutTheSpend",
    "slug": "cutthespend-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#00B77D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cutthespend.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00B77D"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.cutthespend.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "94fa1963-ddfc-486d-bb12-d88d846d688b"
      }
    },
    "plugins": [
      "expo-secure-store"
    ],
    "owner": "arjunan"
  }
}
